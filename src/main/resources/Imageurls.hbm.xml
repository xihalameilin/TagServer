<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.HibernateEntity.Imageurls" table="imageurls" schema="picturetagdatabase">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="taskId">
            <column name="taskID" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="imageUrl">
            <column name="imageURL" sql-type="varchar(80)" length="80" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>