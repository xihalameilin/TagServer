<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.example.demo.HibernateEntity.Options" table="options" schema="picturetagdatabase">
        <id name="optionId">
            <column name="optionId" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="taskId">
            <column name="taskID" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="tagOption">
            <column name="tagOption" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>